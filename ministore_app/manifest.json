{
	"manifest_version": 2,
	"name": "ARTEMIS Ministore",
	"description": "GM lab ministore",
	"version": "0.1",
	"app": {
		"background": {
			"scripts": ["background.js"]
		}
	},
	"permissions": [
		"http://localhost/*",
		"http://*/",
		"http://*/*",
		"webview",
		{"fileSystem": ["write","retainEntries","directory"]},
		"identity",
		"storage",
		"fullscreen"
	],
	"file_handlers": {
			 "text": {
			 	 "types": [
				 	  "text/*"
				 ],
				 "title": "Text editor"
			 }
	 }
}