{
	"name":	"ARTEMIS Ministore",
	"version": "0.1",
	"manifest_version": 2,
	"description": "GM lab ministore",
	"minimum_chrome_version":"30",
	"app": {
	    "background": {
	        "scripts": ["background.js"]
	    }
	},
	"permissions": [
	    "http://localhost/*",
	    "http://*/",
	    "http://*/*",
	    "webview",
	    {"fileSystem": ["write","retainEntries"]},
	    "identity",
	    "storage",
	    "fullscreen",
	    "serial"
	],
	"file_handlers": {
	    "text": {
	        "types": [
		    "text/*"
		],
		"title": "Text editor"
	    }
	}
}